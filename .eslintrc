{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"js": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"prettier/prettier": [
		"error",
		{
			"endOfLine": "auto"
		},
	],
	"plugins": [
		"react"
	],
	"rules": {
		"react/react-in-jsx-scope": "off"
	}
}
